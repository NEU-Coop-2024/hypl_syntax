@top Program { expression* }

expression {
  Identifier |
  Name |
  Application { "(" expression* ")" }
}

NewExpression { @specialize<Identifier, "int"> expression }

@tokens {
  Identifier { $[a-z] $[a-zA-Z0-9_]* } 
  Name { $[A-Z] $[a-zA-Z0-9_]* } 
}

@skip {
  NewExpression
}

@detectDelim
